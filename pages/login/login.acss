/* pages/login/index.wxss */
page{
  background: #fff;
}
.login-formbox{
    width: 100%;
  padding:40rpx 34rpx;
  align-items: center;
  justify-content:space-between;
}
.form-item{
  position:relative;
  display:flex;
  width:100%;
  margin-top: 34rpx;  
}
.form-inp{
  position:relative;
  display:block;
  width:89%;
  font-size:15px;
  padding:20rpx 0 20rpx 10%;
  border:solid #E6E6E6 1px;
  border-radius:4px;
  color:#666;
}

.form-item .inp-ic1{
  position:absolute;
  left:11px;
  top:50%;
  margin-top:-8px;
  background:url(data:image/png;base64,);
  background-size:17px 15px;
  display:block;
  width:17px;
  height:15px;
}
.form-item .inp-ic2{
  position:absolute;
  left:11px;
  top:50%;
  margin-top:-12px;
  background:url(data:image/png;base64,);
  background-size:18px 24px;
  display:block;
  width:18px;
  height:24px;
}
.form-item .inp-ic3{
  position:absolute;
  left:11px;
  top:50%;
  margin-top:-7px;
  background:url(data:image/png;base64,);
  background-size:20px 14px;
  display:block;
  width:20px;
  height:14px;
}
.form-item .inp-ic4{
  position:absolute;
  left:11px;
  top:50%;
  margin-top:-20rpx;
  background:url(data:image/png;base64,);
  background-size:40rpx 40rpx;
  background-repeat: no-repeat;
  display:block;
  width:40rpx;
  height:40rpx;
}
.form-item .inp-erro-fon{
  border-color:#EF3232;
  color:#ef3232;
}
.form-item .erro-icon{
  position:absolute;
  right:11px;
  top:50%;
  margin-top:-8px;
  background:url(data:image/png;base64,);
  background-size:16px 16px;
  display:block;
  width:16px;
  height:16px;

}

.form-submit{
  margin-top:40rpx;
  width:100%;
  line-height:48px;
  background:#1B82D2;
  color:#fff;
  font-size:17px;
  font-weight: bold;
}
.tips-info{
  display:block;
  margin-top:35rpx;
  color:#E20212;
  font-size:14px;
  text-align:center;
}

.form-select-box{  
  display:flex;
  align-items: center;
  justify-content:space-between;
}
.form-select-box .form-tab{
  position:relative;
  width:48%;
  order:1;
  padding:20rpx 0;
  border:solid #E6E6E6 1px;
  border-radius:4px;
  color:#666;
  text-align:center;
  font-size:14px;

}
.form-select-box .form-tab:last-child{
  order:2
}
.form-select-box .form-tab image{
  display:none;
  position:absolute;
  right:0;
  bottom:0;
  width:20px;
  height:20px;
}
.form-select-box .focus{
  border:solid #1B82D2 1px;
  color:#1B82D2;
  background:#FBFDFF;
}
.form-select-box .focus image{
  display:block;
}
navigator{
  width:100%;
}
@media(max-width:399px){
  .form-inp{
    width:88%;
    padding:20rpx 0 20rpx 11%;
  }
}
#code-button{
  position: absolute;
  height: 50rpx;
  line-height: 50rpx;
  padding: 20rpx 0;
  box-sizing: content-box;
  width: 30%;
  right: 0rpx;
  font-size: 24rpx;
  z-index: 11;
  background: #10c637;
  color: #fff;
}
#code-button[type="warn"]{
  background-color:#ccc;
  color: #fff;
}
.mailTips{
  font-size: 30rpx;
  position: absolute;
  color: #c4c4c4;
}
.form-phone{
  width: 55%;
}
.login-formbox input{
  height:50rpx;
}
